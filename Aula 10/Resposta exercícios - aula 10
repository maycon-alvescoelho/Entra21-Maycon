1) 
let i = 0
while (i < 10) {
    console.log("Olá mundo!")
    i++
}

let i = 0
do {
    console.log("Olá mundo!")
    i++
} while (i < 10)

for (let i = 0 ; i < 10; i++) {
    console.log("Olá mundo!")
}

2) 
let i = 0
while (i < 10) {
	console.log(`Estou aparecendo pela ${i++} na tela`)
}

